{
  "version": 2,
  "functions": {
    "api/upload_contract.py": {
      "runtime": "@vercel/python",
      "maxDuration": 300
    },
    "api/**/*.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    { "src": "/api/upload_contract", "dest": "api/upload_contract.py" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
} 